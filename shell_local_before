path_prepend "$HOME/bin"

# Homebrew might not be installed in the default location
brew_prefix=$(brew --prefix)

# Homebrew and Related Tools
path_prepend "$brew_prefix/sbin"
path_prepend "$brew_prefix/bin"
path_prepend "$brew_prefix/opt/coreutils/libexec/gnubin"
path_prepend "$brew_prefix/opt/python/libexec/bin"
path_prepend "$brew_prefix/opt/ruby/bin"
